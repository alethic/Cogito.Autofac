    <ItemGroup>
        <PackageReference Include="Autofac" Version="4.8.1" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="2.0.2" />
        <PackageReference Include="Cogito.Configuration" Version="1.0.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\FileAndServe.Configuration\FileAndServe.Configuration.csproj" />
    </ItemGroup>

</Project>