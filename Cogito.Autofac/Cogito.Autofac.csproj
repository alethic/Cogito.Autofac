<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net47;net462</TargetFrameworks>
        <PackageId>FileAndServe.Autofac</PackageId>
        <Authors>File &amp; Serve</Authors>
        <Company>File &amp; Serve</Company>
        <Product>FileAndServe.Autofac</Product>
        <Description>Provides some Autofac utility classes used by the rest of the Component Framework.</Description>
        <Copyright>File &amp; Serve</Copyright>
        <PackageTags>fsx autofac</PackageTags>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <Version>0.0.1</Version>
