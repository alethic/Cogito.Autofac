<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>netstandard2.0;net47;net462</TargetFrameworks>
        <PackageId>Cogito.Autofac</PackageId>
        <Authors>Jerome Haltom</Authors>
        <Company>Jerome Haltom</Company>
        <Product>Cogito.Autofac</Product>
        <Description>Provides various autofac utilities, and a new unique attribute registration system.</Description>
        <Copyright>Jerome Haltom</Copyright>
        <PackageTags>autofac</PackageTags>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <Version>0.0.1</Version>
        <AssemblyName>Cogito.Autofac</AssemblyName>
        <PackageIconUrl>https://cloverluxussc.blob.core.windows.net/public/cogito-nuget-icon.png</PackageIconUrl>
        <PackageProjectUrl>https://github.com/wasabii/Cogito.Autofac</PackageProjectUrl>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Autofac" Version="4.8.1" />
        <PackageReference Include="Microsoft.Extensions.Configuration.Abstractions" Version="2.1.1" />
        <PackageReference Include="Cogito.Extensions.Configuration" Version="1.1.0-unstable0001" />
        <PackageReference Include="Cogito.Core" Version="3.0.0" />
    </ItemGroup>

</Project>