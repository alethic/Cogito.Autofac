<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.0</TargetFramework>
        <PackageId>Cogito.Autofac.DependencyInjection</PackageId>
        <Authors>Jerome Haltom</Authors>
        <Company>Jerome Haltom</Company>
        <Product>Cogito.Autofac.DependencyInjection</Product>
        <Description>Provides various Autofac related integration to Microsoft.Extensions.DependencyInjection.</Description>
        <Copyright>Jerome Haltom</Copyright>
        <PackageTags>autofac</PackageTags>
        <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
        <Version>0.0.1</Version>
        <PackageProjectUrl>https://github.com/wasabii/Cogito.Autofac</PackageProjectUrl>
        <PackageIconUrl>https://cloverluxussc.blob.core.windows.net/public/cogito-nuget-icon.png</PackageIconUrl>
    </PropertyGroup>

    <ItemGroup>
        <PackageReference Include="Autofac" Version="4.9.2" />
        <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="4.4.0" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="2.2.0" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Cogito.Autofac\Cogito.Autofac.csproj" />
    </ItemGroup>

</Project>